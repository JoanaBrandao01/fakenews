{
    "name": "FakeNews Extension",
    "version": "0.0.1",
    "manifest_version": 3,
    "permissions": [
      "tabs",
      "scripting"
    ],
    "action": {
      "default_popup": "index.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }